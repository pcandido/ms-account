  },
  "homepage": "https://github.com/pcandido/ms-account#readme",
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.2.3",
    "@types/amqplib": "^0.5.17",
    "@types/bcrypt": "^3.0.0",
    "@types/config": "^0.0.38",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/module-alias": "^2.0.0",
    "@types/mongodb": "^3.6.8",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.14.27",
    "@types/supertest": "^2.0.10",
    "@types/validator": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "eslint": "^7.20.0",
    "fake-s3": "^3.1.3",
    "husky": "^5.0.9",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "mock-amqplib": "^1.3.0",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.2.2"
  },
